begin simulation;
  time_stop    [time stop];
  solvertype   1 ;    (newmark)
  on_no_convergence continue ;
  convergence_limits 1E3 1.0 0.7 ;
  logfile ./log/[Case id.].log ;
;  animation ./animation/[Case id.].dat;
;
  begin newmark;
    deltat    0.02;  
  end newmark;
end simulation;
;
